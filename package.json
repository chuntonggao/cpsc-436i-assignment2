    "devDependencies": {
        "@types/classnames": "^2.2.10",
        "@types/node": "^12.12.42",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/uuid": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
